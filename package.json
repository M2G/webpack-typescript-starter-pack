{
  "name": "webpack-typescript-starter-pack",
  "version": "0.0.2",
  "description": "A basic webpack config using TypeScript (and/or JS)",
  "main": "./src/index.ts",
  "private": false,
  "author": "M2G",
  "license": "MIT",
  "keywords": [
    "webpack",
    "typescript",
    "babel",
    "jest",
    "typescript"
  ],
  "homepage": "https://github.com/M2G/webpack-typescript-starter-pack#readme",
  "bugs": {
    "url": "https://github.com/M2G/webpack-typescript-starter-pack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/M2G/webpack-typescript-starter-pack.git"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts, .; exit 0",
    "test:e2e": "node_modules/.bin/nightwatch -c ./tests/e2e/nightwatch.conf.js --env chrome",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "start:dev": "NODE_ENV=development webpack-dev-server --config ./config/development/webpack.config.js --mode development",
    "start:staging": "NODE_ENV=staging webpack-dev-server --config ./config/staging/webpack.config.js --mode staging",
    "start:prod": "NODE_ENV=production webpack-dev-server --config ./config/development/webpack.config.js --mode production",
    "build:dev": "NODE_ENV=development webpack --mode development --config ./config/development/webpack.config.js --progress --watch",
    "build:staging": "NODE_ENV=staging webpack --mode staging --config ./config/staging/webpack.config.js --progress",
    "build:production": "NODE_ENV=production webpack --mode production --config ./config/production/webpack.config.js --progress",
    "publish:npm": "rm -rf ./build && NODE_ENV=production webpack --mode production --config ./config/publish/webpack.config.js --progress"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "autoprefixer": "^9.8.5",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.11",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.3.0",
    "http-proxy-middleware": "^1.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.1.0",
    "jest-cli": "^26.1.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-resolve": "26.1.0",
    "jest-watch-typeahead": "0.6.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-color-rebeccapurple": "^4.0.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^2.0.5",
    "resolve": "^1.17.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.7",
    "ts-jest": "^26.1.3",
    "ts-loader": "^8.0.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-dotenv-plugin": "^2.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "dependencies": {
    "cssnano": "^4.1.10",
    "eslint-webpack-plugin": "^2.5.2"
  },
  "files": [
    "build/*"
  ]
}
